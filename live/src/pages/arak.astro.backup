    <script is:inline>
        function calculateCost() {
            console.log('Calculate cost function called');
            
            const areaInput = document.getElementById('area');
            const paverTypeSelect = document.getElementById('paverType');
            const projectTypeSelect = document.getElementById('projectType');
            
            console.log('Area input:', areaInput);
            console.log('Area value:', areaInput ? areaInput.value : 'not found');
            
            if (!areaInput || !paverTypeSelect || !projectTypeSelect) {
                console.error('One or more form elements not found');
                alert('Hiba t√∂rt√©nt a form elemek bet√∂lt√©s√©ben!');
                return;
            }
            
            const area = parseFloat(areaInput.value);
            const paverType = paverTypeSelect.value;
            const projectType = projectTypeSelect.value;
            
            console.log('Values:', { area, paverType, projectType });
            
            if (!area || area <= 0 || isNaN(area)) {
                alert('K√©rj√ºk adja meg a ter√ºlet m√©ret√©t (pozit√≠v sz√°m)!');
                return;
            }
            
            let basePrice;
            switch (paverType) {
                case 'basic':
                    basePrice = 13500; // Average of 12000-15000
                    break;
                case 'premium':
                    basePrice = 19000; // Average of 16000-22000
                    break;
                case 'exclusive':
                    basePrice = 30000; // Average of 25000-35000
                    break;
                default:
                    basePrice = 13500;
            }
            
            // Project type multiplier
            let multiplier = 1;
            switch (projectType) {
                case 'driveway':
                    multiplier = 1.1; // Higher load bearing requirements
                    break;
                case 'terrace':
                    multiplier = 1.0;
                    break;
                case 'walkway':
                    multiplier = 0.9; // Simpler installation
                    break;
                case 'patio':
                    multiplier = 1.05;
                    break;
                default:
                    multiplier = 1.0;
            }
            
            const totalCost = Math.round(area * basePrice * multiplier);
            const minCost = Math.round(totalCost * 0.9);
            const maxCost = Math.round(totalCost * 1.1);
            
            console.log('Calculated costs:', { totalCost, minCost, maxCost });
            
            const costRangeElement = document.getElementById('costRange');
            const resultElement = document.getElementById('result');
            
            if (!costRangeElement || !resultElement) {
                console.error('Result elements not found');
                alert('Hiba t√∂rt√©nt az eredm√©ny megjelen√≠t√©s√©ben!');
                return;
            }
            
            costRangeElement.textContent = 
                new Intl.NumberFormat('hu-HU').format(minCost) + ' - ' + 
                new Intl.NumberFormat('hu-HU').format(maxCost) + ' Ft';
            
            resultElement.classList.remove('hidden');
            
            console.log('Result displayed');
            
            // Scroll to result
            setTimeout(() => {
                resultElement.scrollIntoView({ behavior: 'smooth', block: 'center' });
            }, 100);
        }
    </script>
